<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.github.krtonga.busbabe.MapActivityFragment"
    tools:showIn="@layout/activity_map">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:mapbox_cameraTargetLat="-6.799156"
        mapbox:mapbox_cameraTargetLng="39.258505"
        mapbox:mapbox_styleUrl="@string/mapbox_style"
        mapbox:mapbox_cameraZoom="10"/>

</android.support.constraint.ConstraintLayout>
